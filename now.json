{
    "name": "nowV2test",
    "alias": "nowV2test",
    "version": 2,
    "regions": ["all"],
    "public": true,
    "builds": [
        { "src": "./front/next.config.js", "use": "@now/next" },
        { "src": "./back/service/**/*.ts", "use": "@now/node-server@canary" },
        { "src": "./back/api/**/*.ts", "use": "@now/node@canary" }
    ],
    "routes": [
        { "src": "/socket.io/", "dest": "/back/service/ping.ts" },
        { "src": "/api/echo/(?<id>[^/]*)", "dest": "back/api/echo.ts?id=$id" },
        { "src": "/(.*)", "dest": "/front/$1", "headers": { "x-request-path": "$1" }}
    ]
}

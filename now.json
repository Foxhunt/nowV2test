{
    "name": "nowV2test",
    "alias": "nowV2test",
    "version": 2,
    "regions": ["all"],
    "public": true,
    "builds": [
        { "src": "package.json", "use": "@now/next" },
        { "src": "./back/**/*.ts", "use": "@now/node-server" },
        { "src": "./api/**/*.ts", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/", "dest": "/pages" },
        { "src": "/socket.io/", "dest": "/back/server.js" },
        { "src": "/api/echo/(?<id>[^/]*)", "dest": "/api/echo.js?id=$id" }
    ]
}

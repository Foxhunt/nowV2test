{
    "name": "nowV2test",
    "alias": "nowV2test",
    "version": 2,
    "regions": ["all"],
    "public": true,
    "builds": [
        { "src": "front/*", "use": "@now/static" },
        { "src": "back/*", "use": "@now/node-server" },
        { "src": "api/*", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/", "dest": "/front" },
        { "src": "/socket.io", "dest": "/back/ping_server.js" },
        { "src": "/peerjs", "dest": "/back/peer_server.js" },
        { "src": "/api/echo/(?<id>[^/]*)", "dest": "/api/echo.js?id=$id" }
    ]
}
